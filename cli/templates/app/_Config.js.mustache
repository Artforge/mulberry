dojo.provide('toura._Config');

mulberry._Config = {{{base_config}}};

// force all images to come from S3. this
// is mainly used by MAP for the preview window.
mulberry.forceLocal = {{force_local}};
mulberry.forceStreaming = {{force_streaming}};

// skips checking for newer remote version.
// useful for speeding up non-data dev.
mulberry.skipVersionCheck = {{skip_version_check}};

mulberry.features = {
  multiLineChildNodes : false,
  sharing : true,
  favorites : true,
  fontSize : true,
  siblingNav : {{sibling_nav}},
  debugToolbar : {{debug}},
  debugPage : true
};

{{postscript}}
