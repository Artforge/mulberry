%ul.component.child-node-text
  :each c in children
    %li{ class: c.id }
      :if c.bodyText && c.bodyText.length > 0 && c.bodyText[0].body != ""
        %h3.child-node-title =c.name
        .child-node-body-text =c.bodyText[0].body
      :if !c.bodyText || c.bodyText[0].body == ""
        %a{ href : '#/node/' + c.id }= c.name
        